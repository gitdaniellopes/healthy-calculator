<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.daniel.healthycalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".ui.ListCalcActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".ui.FcmActivity"
            android:theme="@style/FcmTheme" />
        <activity
            android:name=".ui.TmbActivity"
            android:theme="@style/TbmTheme" />
        <activity
            android:name=".ui.ImcActivity"
            android:theme="@style/imcTheme" />
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/MainTheme" />
        <activity
            android:name=".ui.SplashScreenActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>